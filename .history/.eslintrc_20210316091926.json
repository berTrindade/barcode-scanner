{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    },
    "settings": {
        "import/resolver": {
            "typescript": {},
            "babel-plugin-root-import": {
            "rootPathPrefix": "~",
            "rootPathSuffix": "src"
            }
        }
    }
}
